cd "$1" || exit 1
echo "-----> Updating geoip2 database"
pyhton manage.py update_geoip2_database
echo "-----> Compiling SCSS"
python manage.py compilescss
echo "-----> Collecting static files"
python manage.py collectstatic --noinput -i static_src
echo "-----> Installing Not deps packages"
pip install django-session-header --no-dependencies
echo "-----> Deleting static src folder"
rm -rf ./static_src
